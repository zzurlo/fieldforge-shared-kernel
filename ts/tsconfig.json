{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "../lib",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "models/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}